<table>
<name>vtiger_cbdecisiontable</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbdecisiontable` (
  `cbdecisiontableid` int(11) NOT NULL,
  `cbdecisiontable_no` varchar(56) default NULL,
  `cbdtname` varchar(210) NOT NULL,
  `cbdtdecides` varchar(250) default NULL,
  `cbdtgroup` varchar(250) default NULL,
  `sequence` varchar(20) default NULL,
  `condstr1` varchar(250) default NULL,
  `condstr2` varchar(250) default NULL,
  `condstr3` varchar(250) default NULL,
  `condstr4` varchar(250) default NULL,
  `condnum1` decimal(25,6) default NULL,
  `condnum2` decimal(25,6) default NULL,
  `condnum3` decimal(25,6) default NULL,
  `condnum4` decimal(25,6) default NULL,
  `valstr1` varchar(250) default NULL,
  `valstr2` varchar(250) default NULL,
  `valstr3` varchar(250) default NULL,
  `valstr4` varchar(250) default NULL,
  `valnum1` decimal(25,6) default NULL,
  `valnum2` decimal(25,6) default NULL,
  `valnum3` decimal(25,6) default NULL,
  `valnum4` decimal(25,6) default NULL,
  PRIMARY KEY (`cbdecisiontableid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
