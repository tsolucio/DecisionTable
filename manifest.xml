<table>
<name>vtiger_cbdecisiontable</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbdecisiontable` (
  `cbdecisiontableid` int(11) NOT NULL,
  `cbdecisiontable_no` varchar(56) default NULL,
  `cbdtname` varchar(210) NOT NULL,
  `cbdtdecides` varchar(250) default NULL,
  `cbdtgroup` varchar(250) default NULL,
  `sequence` varchar(20) default NULL,
  `cond1` varchar(250) default NULL,
  `cond2` varchar(250) default NULL,
  `cond3` varchar(250) default NULL,
  `cond4` varchar(250) default NULL,
  `val1` varchar(250) default NULL,
  `val2` varchar(250) default NULL,
  `val3` varchar(250) default NULL,
  `val4` varchar(250) default NULL,
  PRIMARY KEY (`cbdecisiontableid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
